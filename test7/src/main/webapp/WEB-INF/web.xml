<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>IOServlet</servlet-name>
		<servlet-class>com.academy.IOServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>IOServlet</servlet-name>
		<url-pattern>/IOServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>StorageEditor</servlet-name>
		<servlet-class>com.academy.StorageEditor</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StorageEditor</servlet-name>
		<url-pattern>/StorageEditor</url-pattern>
	</servlet-mapping>
</web-app>